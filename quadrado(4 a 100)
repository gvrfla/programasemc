#include <stdio.h>

int main()
{
    int n = 0;
    do {
        printf ("Escreva o numero de vezes que sera impresso --> ");
        scanf ("%d", &n);
        } while (n < 4 || n > 100);
    do
      {
        printf ("\nXXXXX\n");
        printf ("X   X\n");
        printf ("X   X\n");
        printf ("X   X\n");
        printf ("XXXXX\n");
        n--;
      } while (n > 0);  
getch();
return 0;
}
